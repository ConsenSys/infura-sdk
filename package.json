{
  "name": "@infura/sdk",
  "version": "0.1.0",
  "description": "Infura SDK: easily manage, interact with and read smart contract on Mainnet, testnet and local",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "test": "jest --testPathPattern=test/",
    "test:e2e": "jest --testPathPattern=e2e/",
    "test:coverage": "jest --coverage --coverageReporters='json-summary'",
    "prettier:format": "prettier -- write \"*.js\" ",
    "prettier:check": "prettier --check .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ConsenSys/nft-sdk.git"
  },
  "author": "Consensys Inc",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ConsenSys/nft-sdk/issues"
  },
  "homepage": "https://github.com/ConsenSys/nft-sdk#readme",
  "keywords": [
    "infura",
    "sdk",
    "ethereum",
    "evm",
    "blockchain",
    "smart contracts",
    "dapps",
    "solidity",
    "nft",
    "web3",
    "tooling"
  ],
  "dependencies": {
    "axios": "^0.27.2",
    "dotenv": "^16.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "babel-jest": "^28.1.0",
    "eslint": "^8.14.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "ethers": "^5.6.5",
    "ganache": "^7.0.0",
    "husky": "^7.0.0",
    "jest": "^28.1.0",
    "lint-staged": "^12.4.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  }
}
